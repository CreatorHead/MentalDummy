<div class="container container-table">
	<div class='row vertical-center-row'>
		<div class="panel panel-default">
			<div class="panel-heading"><strong>Pass Key</strong></div>
			<div class="panel-body">
				<form action="#" method="post">
					<div class="form-group">
						<p id=passIdPara>
							<input id='passInput' type="text" class="form-control"
								placeholder="Enter Pass Key" name="passkey" required>
						</p>
					</div>
					<div class="form-group">
						<button id='btnValidate' type="submit"
							class="btn btn-default elementColor">Submit</button>
					</div>
				</form>
			</div>
		</div>
	</div>
	<!-- row -->
</div>